    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "cblink-service/kernel": "^0.2.0",
        "cblink/laravel-dto": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.5",
        "mockery/mockery": "^0.9|^1.3",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Cblink\\Service\\Payment\\": "src"
        }
    },
    "autoload-dev": {
